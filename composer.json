    "require": {
        "hiqdev/hiapi": "v2.x-dev",
        "ramsey/uuid": "^3.7",
        "filestack/filestack-php": "^1.1",
        "claviska/simpleimage": "^3.3",
        "hiqdev/hifile-sql": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev": "<2.0 || dev-master",
        "hiqdev/hidev-php": "<2.0 || dev-master",
        "hiqdev/hidev-hiqdev": "<2.0 || dev-master"
    },
    "extra": {
        "config-plugin": {
            "params": "src/config/params.php",
            "common": "src/config/common.php",
            "console": "src/config/console.php",
            "web": "src/config/web.php"
        }
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\hifile\\api\\": "src"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
